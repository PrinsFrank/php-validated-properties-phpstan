{
    "name": "prinsfrank/php-validated-properties-phpstan",
    "description": "PHPStan extension for the php-validated-properties package",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "phpstan/phpstan": "^1.4"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.2"
    },
    "autoload": {
        "psr-4": {
            "PrinsFrank\\PhpValidatedPropertiesPhpStan\\": "src/"
        }
    },
    "scripts": {
        "fix": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    }
}
